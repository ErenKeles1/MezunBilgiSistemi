<body>
    <section class="ftco-section">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-md-6 text-center mb-5">

                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-7 col-lg-5">
                    <div class="login-wrap p-4 p-md-5">
                        <div class="icon d-flex align-items-center justify-content-center">
                            <span class="fa fa-user-o"></span>
                        </div>
                        <h3 class="text-center mb-4">Giriş Yap</h3>
                        @using (Html.BeginForm("AdminRoll", "AdminRole", FormMethod.Post))
                        {


                            <div class="form-group">
                                @Html.TextBoxFor(m => m.registerEmail, new { @class = "form-control", placeholder = "email" })
                            </div>
                            <div class="form-group d-flex">
                                @Html.PasswordFor(m => m.registerPassword, new { @class = "form-control", placeholder = "password" })
                            </div>
                            <div >
                                @Html.TextBoxFor(m => m.registerRoll, new {@class="form-control",placeholder="rol giriniz"})
                                <br />
                            </div>
                            <div class="form-group">

                                <button type="submit" class=" btn btn-primary ">Admin-Giriş Yap</button>
                            </div>

                            <div class="form-group d-md-flex">


                                <div><a href="/Login/LoginVerification" class="btn btn-warning"> Admin Değilseniz Giriş Yap kısmına gidiniz</a> </div>
                                @ViewBag.Message
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="~/Views/Login/js/jquery.min.js"></script>
    <script src="~/Views/Login/js/popper.js"></script>
    <script src="~/Views/Login/js/bootstrap.min.js"></script>
    <script src="~/Views/Login/js/main.js"></script>
